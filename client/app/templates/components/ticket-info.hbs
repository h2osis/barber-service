<div class="portlet-title">
    <div class="caption">
        <i class="icon-info font-green-sharp"></i>
        <span class="caption-subject font-green-sharp">Информация о записи</span>
    </div>
</div>
<div class="portlet-body ticket-portlet-body">
    <div class="ticket-info">
        {{ticket-info-master}}
        {{ticket-info-services}}
        {{ticket-info-cost}}        
        {{ticket-info-date}}
        {{ticket-info-client}}
    </div>    
    <div class="ticket-submit-btn-wrapper">
        {{ticket-form ticket=ticket}}
    </div>            
</div>